namespace BassicProgram
{
    internal class Program
    {
        static void Main(string[] args)
        {
            ArrayList tem= new ArrayList();
            tem.Add(200);
            tem.Add(500);
            for(int i = 0; i < tem.Count; i++)
            {
                Console.WriteLine(tem[i]);
            }
          

            Console.ReadLine();
        }
    }
}
